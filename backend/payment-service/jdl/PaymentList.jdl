entity PaymentList {
  status String,
  type String,
  code String required,
  name String,
  paymentProviderId Integer,
  paymentFeePercentage BigDecimal,
  paymentFeeNominal BigDecimal,
  paymentNotes String,
  imageUrl String,
  howToPay TextBlob,
  note TextBlob,
  createdAt Instant,
  createdBy Integer required,
  updatedAt Instant,
  updatedBy Integer required
}

// You can define relationships here if needed
relationship ManyToOne {
  PaymentList{paymentProviderId} to PaymentProvider
}
